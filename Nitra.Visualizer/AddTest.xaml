<Window x:Class="Nitra.Visualizer.AddTest"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="AddTest" Height="169" Width="537" ResizeMode="NoResize" FontSize="16" WindowStartupLocation="CenterOwner" ShowInTaskbar="False">
    <Grid>
    <Label Content="_Test name" Height="28" HorizontalAlignment="Left" Margin="12,29,0,0" Name="label1" VerticalAlignment="Top" FontSize="16" />
    <TextBox Height="28" HorizontalAlignment="Right" Margin="0,29,10,0" Name="_testName" VerticalAlignment="Top" Width="390" FontSize="16" />
    <Button Content="_OK" Height="31" HorizontalAlignment="Right" Margin="0,0,111,12" Name="_okButton" VerticalAlignment="Bottom" Width="95" FontSize="16" IsDefault="True" Click="_okButton_Click" />
    <Button Content="_Cancel" Height="31" HorizontalAlignment="Right" Margin="0,0,10,12" Name="_cancelButton" VerticalAlignment="Bottom" Width="95" FontSize="16" IsCancel="True" />
  </Grid>
</Window>
