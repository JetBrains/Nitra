using N2;
using System.Console;

module Program
{
  Main() : void
  {
    Test("x");
    Test("y");
  }
  
  Test(text : string) : void
  {
    def source = SourceSnapshot(text);
    def parserHost = ParserHost();
    def result = Foo.Start(source, parserHost);
    if (result.IsSuccess)
      WriteLine("OK");
    else
      WriteLine("Parse fail");
  }
}
