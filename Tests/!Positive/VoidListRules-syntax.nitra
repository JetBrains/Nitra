syntax module M
{
  using Whitespaces;
  using Identifiers;

  [StartRule]
  syntax Start1 = (A; x)+ !Any
  {
    void   x = ",";
    syntax A = "A";
  }
  
  [StartRule]
  syntax Start2 = (A; x)+ !Any
  {
    syntax x = ",";
    void   A = "A";
  }

  [StartRule]
  syntax Start3 = (A; x)+ !Any
  {
    void x = ",";
    void A = "A";
  }
}
