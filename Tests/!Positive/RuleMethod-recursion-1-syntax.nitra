using Nitra;

syntax module RecursionTest1
{
  using Whitespaces;

  [StartRule, ExplicitSpaces] syntax Start = s A !Any
  {
    Foo() : int = A.Foo();
  }

  syntax A = "a" B
  {
    Foo() : int = B.Foo(this);
  }

  syntax B = "b"
  {
    Foo(owner : RecursionTest1Ast.A) : int = owner.Foo();
  }
}
