syntax module M
{
  using Whitespaces;

  [StartRule]
  syntax Start = A !Any
  {
    Value() : int = A.Value();
  }
  
  marker K1;

  syntax A
  {
    Value() : int;

    | X1 = "x1"                        { override Value = 1; }
    | X2 = "x2"                        { override Value = 2; }
    | Y = P=(A "+") A    precedence 10 { override Value = P[0].Value() + A.Value(); }
    | Z = Q=(K1 A) "*" A precedence 20 { override Value = Q.Value() + A.Value(); }
  }
}