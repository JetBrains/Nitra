syntax module M1
{
  using Whitespaces;

  syntax X
  {
    Field1() : int = 0;

    | Y = "y"
      {
        override Field2 = 1; //E:No 'Field2' rule method to override
      }

    syntax Z = "z"
    {
      override Field1 = 1; //E:An rule method can be overridden only in extending rules
    }
  }
}
